<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dfc.ind.gateway.mapper.FiltersterMapper">

    <select id="lists" parameterType="com.dfc.ind.gateway.entity.FiltersterVo"  resultType="com.dfc.ind.gateway.entity.FiltersterVo">
         select * from c_apic.${pathtype}
         <where>
             <if test="movUrl != null and movUrl !=''">
                 mov_url like   concat('%',#{movUrl},'%')
             </if>
         </where>
         order By create_time desc
        <if test="pageSize!=0">
            LIMIT #{pageNum},#{pageSize}
        </if>
    </select>

    <insert id="addrebo"  parameterType="com.dfc.ind.gateway.entity.FiltersterVo" >
        insert into ${pathtype}
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="movUrl != null">mov_url,</if>
            <if test="roleId != null">role_id,</if>
            <if test="aisleType != null">aisle_type,</if>
            <if test="menuName != null">menu_name,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="updateBy != null">update_by</if>

        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="movUrl != null">#{movUrl},</if>
            <if test="roleId != null">#{roleId},</if>
            <if test="aisleType != null">#{aisleType},</if>
            <if test="menuName != null">#{menuName},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="updateBy != null">#{updateBy}</if>
        </trim>



    </insert>



</mapper>
